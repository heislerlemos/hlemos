  <body>
    <!--The div element for the map -->

    <div id="progression">

          <div class="title is-1">
      <center>
          Pagina em manutenção
          </center>
        </div>
<progress class="progress is-small is-primary" max="50">15%</progress>
<div class="has-text-centered is-family-primary has-text-weight-bold"> 
</div>
